    <Attribute name="GeneralOut3"         type="PARAM"    source="PHOTRON_EXT_OUT_3_SIG"        datatype="INT"        description="General Out 3"/>
    <Attribute name="GeneralOut3Str"      type="EPICS_PV" source="pho:cam1:ExtOut3_RBV"         dbrtype="DBR_STRING"  description="General Out 3 String"/>
    <Attribute name="TrigTTLOut"          type="PARAM"    source="PHOTRON_EXT_OUT_4_SIG"        datatype="INT"        description="Trig TTL Out"/>
    <Attribute name="TrigTTLOutStr"       type="EPICS_PV" source="pho:cam1:ExtOut4_RBV"         dbrtype="DBR_STRING"  description="Trig TTL Out String"/>
